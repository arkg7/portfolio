{
  "name": "personal-webpage",
  "version": "0.1.0",
  "description": "Simple static webpage served from ./webpage",
  "private": false,
  "scripts": {
    "dev": "npx live-server ./webpage --port=3001 --host=0.0.0.0 --open=index.html",
